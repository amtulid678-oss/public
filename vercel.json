{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.css",
      "use": "@vercel/static"
    },
    {
      "src": "*.js",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/chat",
      "dest": "/api/index.js"
    },
    {
      "src": "/chat-with-file",
      "dest": "/api/index.js"
    },
    {
      "src": "/appointments",
      "dest": "/api/index.js"
    },
    {
      "src": "/test",
      "dest": "/api/index.js"
    },
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "GOOGLE_API_KEY": "AIzaSyC63CwDLGDByxWmUXVjYRb-35xks2kxIFg"
  }
}